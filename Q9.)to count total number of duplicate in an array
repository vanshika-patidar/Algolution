#include<stdio.h>

void main()
{
    int a[10] = {5,4,5,1,6,7,6,8,9,1};
    int count = 0;
    printf("The duplicate array index is: ");
    for(int i=0;i<=10;i++)
    {
        for(int j=i+1;j<=10;j++)
        {
            if(a[i]==a[j])
            {
               printf("%d ",i); 
               count+=1;
            }
        }
    }
    printf("\nThe duplicate array count is: %d ",count);
}

#include <stdio.h>
void RightTriangle(int rows) {
    for (int i = 1; i <= rows; i++) {
        for (int j = 1; j <= i; j++) {
            printf("*");
        }
        printf("\n");
    }
}

int main() {
    int rows;

    printf("Enter number of rows: ");
    scanf("%d", &rows);
    RightTriangle(rows);
    return 0;
}


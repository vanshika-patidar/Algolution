 #include<stdio.h>
 void main()
 {
     int a[5]={1,2,3,4,5};
     printf("array is:");
     for(int i=0;i<5;i++)
     {
        printf("%d ",a[i]);
     }
     int target;
     printf("\nenter number to find:");
     scanf("%d",&target);
     for(int i=0;i<5;i++)
     {
       if(target==a[i])
       {
        printf("index:%d",i);
        break;
       }
      else {
          printf("-1");
          break;
     }
    }
}


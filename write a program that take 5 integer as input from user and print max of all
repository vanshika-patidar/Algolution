 #include<stdio.h>
 void main()
 {
     int x;
     printf("enter size of array:");
     scanf("%d",&x);
     int a[x];
     printf("enter elements:\n");
     for(int i=0;i<x;i++)
     {
         scanf("%d",&a[i]);
     }
     int max=a[0];
     for(int i=0;i<x;i++)
     {
         if(a[i]>max)
         {
            max=a[i];
         }
     }
     printf("max:%d",max);
 }
